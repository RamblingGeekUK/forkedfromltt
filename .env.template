# Copy this file to .env and fill in your actual values

# YouTube API key for fetching latest videos
YOUTUBE_API_KEY=your_youtube_api_key_here

# Cloudflare Turnstile secret for CAPTCHA verification
CLOUDFLARE_TURNSTILE_SECRET=your_cloudflare_turnstile_secret_here

# Email configuration for contact form
CONTACT_EMAIL_USER=your_email@gmail.com
CONTACT_EMAIL_PASS=your_app_password_here

# Server port (optional, defaults to 3000)
PORT=3000